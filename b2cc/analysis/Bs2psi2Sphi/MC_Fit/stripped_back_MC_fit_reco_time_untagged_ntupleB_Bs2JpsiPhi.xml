<RapidFit>
	<ParameterSet>
		// True Physics Parameters for signal

		<PhysicsParameter>
			<Name>F_s</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>delta_s</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>Csp</Name>
			<Value>1.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>mistagP1</Name>
			<Value>1.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagP0</Name>
			<Value>0.355</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagSetPoint</Name>
			<Value>0.355</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaP1</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaP0</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaSetPoint</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.6653</Value>
			<Minimum>0.6</Minimum>
			<Maximum>0.75</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.0917</Value>
			<Minimum>-0.2</Minimum>
			<Maximum>0.2</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.25</Value>
			<Minimum>0.</Minimum>
			<Maximum>0.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.51</Value>
			<Type>Free</Type>
			<Minimum>0.</Minimum>
			<Maximum>0.8</Maximum>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>3.3</Value>
			<StepSize>0.05</StepSize>
			<Maximum>6.3</Maximum>
			<Minimum>-6.3</Minimum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>3.07</Value>
			<Minimum>-6.3</Minimum>
			<Maximum>6.3</Maximum>
			<StepSize>0.05</StepSize>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Minimum>-3.3</Minimum>
			<Maximum>3.3</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.8</Value>
			<Type>Fixed</Type>
			<Unit>ps-1</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>0.07</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>lambda</Name>
			<Value>1.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sfBarOffset</Name>
			#<Value>1.2789</Value>
			#<Value>1.2412</Value> #MC2011
			<Value>1.2795</Value> #MC2012
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sfBarSlope</Name>
			#<Value>1.11</Value>
			#<Value>1.96</Value> #MC2011
			<Value>1.06</Value> #MC2012
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sfSigmaOffset</Name>
			#<Value>0.2338</Value>
			#<Value>0.2512</Value> #MC2011
			<Value>0.2344</Value> #MC2012
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sfSigmaSlope</Name>
			#<Value>3.02</Value>
			#<Value>3.14</Value> #MC2011
			<Value>2.89</Value> #MC2012
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sigmaBar</Name>
			#<Value>0.033</Value>
			#<Value>0.0329</Value> #MC2011
			<Value>0.03355</Value> #MC2012
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>timeResFraction</Name>
			#<Value>0.174</Value>
			#<Value>0.180</Value> #MC2011
			<Value>0.170</Value> #MC2012
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

               <PhysicsParameter>
                        <Name>timeResMu</Name>
                        <Value>-0.00333</Value>
                        <Type>Fixed</Type>
                </PhysicsParameter>

	</ParameterSet>


	//==========================================
	// Specify which minimiser to use.

	<Minimiser>
		<MinimiserName>Minuit</MinimiserName>
		<MaxSteps>100000</MaxSteps>
		<GradTolerance>0.01</GradTolerance>
		<Quality>0</Quality>
	</Minimiser>

	<FitFunction>
		<FunctionName>NegativeLogLikelihoodThreaded</FunctionName>
		<Threads>8</Threads>
		<SetIntegratorTest>True</SetIntegratorTest>
		<WeightName>weight</WeightName>
		<OffSetNLL>True</OffSetNLL>
	</FitFunction>

	<NumberRepeats>1</NumberRepeats>

	<CommonPDF>
		<PDF>
			<Name>Bs2JpsiPhi_Signal_v7</Name>
			<ConfigurationParameter>UseHelicityBasis:True</ConfigurationParameter>
                        <ConfigurationParameter>AngularAcceptanceIgnoreNumerator:True</ConfigurationParameter>			
			<ConfigurationParameter>UseTimeAcceptance:True</ConfigurationParameter>
			<ConfigurationParameter>UseEventResolution:True</ConfigurationParameter>
			<ConfigurationParameter>ResolutionModel:Phis2012ResolutionModel</ConfigurationParameter>
		</PDF>
	</CommonPDF>

	<CommonPhaseSpace>
		<PhaseSpaceBoundary>
			<Observable>
				<Name>time</Name>
				<Minimum>0.3</Minimum>
				<Maximum>14.0</Maximum>
				<Unit>ps</Unit>
			</Observable>
			<Observable>
				<Name>helcosthetaK</Name>
				<TF1>helcosthetaK</TF1>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>helphi</Name>
				<TF1>(helphi+TMath::Pi()-(helphi>0.)*(2.*TMath::Pi()))</TF1>
				<Minimum>-3.1416</Minimum>
				<Maximum>3.1416</Maximum>
				<Unit>rad</Unit>
			</Observable>
			<Observable>
				<Name>helcosthetaL</Name>
				<TF1>helcosthetaL</TF1>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>tag</Name>
				<TF1>0*trueid/531.</TF1>
				<Value>1.</Value>
				<Value>0.</Value>
				<Value>-1.</Value>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>mistag</Name>
				<TF1>0.5</TF1>
				<Minimum>0.</Minimum>
				<Maximum>0.5</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>eventResolution</Name>
				<TF1>sigmat</TF1>
				<Minimum>0.0</Minimum>
				<Maximum>0.12</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>weight</Name>
                <TF1>1.</TF1>
				#<TF1>B_velo_eff_weight_with_trigger</TF1>	
                #<TF1>weight_dimuon_trigger</TF1>	
				<Minimum>0</Minimum>
				<Maximum>10</Maximum>
				<Unit> </Unit>
			</Observable>
		</PhaseSpaceBoundary>
	</CommonPhaseSpace>

	<ToFit>
		<CommonPDF>True</CommonPDF>
                <PDFConfigurator>
                    <ConfigurationParameter>AngularAcceptanceFile:ferguson/acceptance_weights_and_histos_2012_12102015.root</ConfigurationParameter>
                    <ConfigurationParameter>TimeAcceptanceFile:2013-3fbAnalysis/Bs_HltPropertimeAcceptance_Data_2012_40bins_Hlt1DiMuon_Hlt2DiMuonDetached_Reweighted.txt</ConfigurationParameter>
                </PDFConfigurator>
		<DataSet>
			<Source>File</Source>
            <FileName>/Home/gcowan1/lhcb/lhcb/b2cc/analysis/Bs2psi2Sphi/data/Bs2JpsiPhi_MC2012_ntupleB_20130904.root</FileName>
            <NTuplePath>DecayTree</NTuplePath>
            <CutString>sel_cleantail==1 && sel_clonecut==1 && sel_qualitycut==1 && sel_daughter1==1 && 60>bkgcat && triggerDecisionUnbiased==1</CutString>
            <NumberEvents>1000000000</NumberEvents>
			<CommonPhaseSpace>
			</CommonPhaseSpace>

		</DataSet>
	</ToFit>

	<ToFit>
        <CommonPDF>True</CommonPDF>
                <PDFConfigurator>
                    <ConfigurationParameter>AngularAcceptanceFile:ferguson/acceptance_weights_and_histos_2012_12102015.root</ConfigurationParameter>
                    #<ConfigurationParameter>TimeAcceptanceFile:2013-3fbAnalysis/Bs_HltPropertimeAcceptance_Data_2012_40bins_Hlt1TrackAndTrackMuonExcl_Hlt2DiMuonDetached.txt</ConfigurationParameter>
                    #<ConfigurationParameter>TimeAcceptanceFile:ferguson/Bs_HltPropertimeAcceptance_Data_2012_40bins_Hlt1TrackAndTrackMuonExcl_Hlt2DiMuonDetached_sel.txt</ConfigurationParameter>
                    <ConfigurationParameter>TimeAcceptanceFile:MC2012_biased_efficiency_40bins.txt</ConfigurationParameter>
                </PDFConfigurator>
		<DataSet>
			<Source>File</Source>
            <FileName>/Home/gcowan1/lhcb/lhcb/b2cc/analysis/Bs2psi2Sphi/data/ferguson/noonecand/MC2012_psi2s_allcuts.root</FileName>
            <NTuplePath>DecayTree</NTuplePath>
            <CutString>triggerDecisionBiasedExcl_2012==1</CutString>
			<NumberEvents>0000000</NumberEvents>
			<CommonPhaseSpace>
			</CommonPhaseSpace>

		</DataSet>
	</ToFit>

    <Output>
            <ComponentProjection>
            <Name>time</Name>
            <XTitle>t [ps]</XTitle>
            <LogY>True</LogY>
            <DrawPulls>True</DrawPulls>
            <Ymin>1</Ymin>
            <DataBins>80</DataBins>
            <ColorKey>2</ColorKey>
        </ComponentProjection> 
    </Output>


</RapidFit>


