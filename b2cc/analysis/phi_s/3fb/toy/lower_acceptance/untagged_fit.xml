<RapidFit>
	<ParameterSet>
		// True Physics Parameters for signal

		<PhysicsParameter>
			<Name>F_s</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>delta_s</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>Csp</Name>
			<Value>1.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>mistagP1</Name>
			<Value>1.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagP0</Name>
			<Value>0.355</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagSetPoint</Name>
			<Value>0.355</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaP1</Name>
			<Value>0.</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaP0</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>
		<PhysicsParameter>
			<Name>mistagDeltaSetPoint</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.6653</Value>
			<Minimum>0.6</Minimum>
			<Maximum>0.75</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.0917</Value>
			<Minimum>0.01</Minimum>
			<Maximum>0.15</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.249</Value>
			<Minimum>0.</Minimum>
			<Maximum>0.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.5213</Value>
			<Type>Free</Type>
			<Minimum>0.</Minimum>
			<Maximum>0.8</Maximum>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>3.3</Value>
			<StepSize>0.05</StepSize>
			<Maximum>6.3</Maximum>
			<Minimum>-6.3</Minimum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>3.07</Value>
			<Minimum>-6.3</Minimum>
			<Maximum>6.3</Maximum>
			<StepSize>0.05</StepSize>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Minimum>-3.3</Minimum>
			<Maximum>3.3</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.8</Value>
			<Type>Fixed</Type>
			<Unit>ps-1</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>0.07</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>lambda</Name>
			<Value>1.</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

	</ParameterSet>


	//==========================================
	// Specify which minimiser to use.

	<Minimiser>
		<MinimiserName>Minuit</MinimiserName>
		<MaxSteps>100000</MaxSteps>
		<GradTolerance>0.01</GradTolerance>
		<Quality>0</Quality>
	</Minimiser>

	<FitFunction>
		<FunctionName>NegativeLogLikelihoodThreaded</FunctionName>
		<Threads>8</Threads>
		<SetIntegratorTest>False</SetIntegratorTest>
		<OffSetNLL>True</OffSetNLL>
	</FitFunction>

	<NumberRepeats>1</NumberRepeats>

	<CommonPDF>
		<PDF>
			<Name>Bs2JpsiPhi_Signal_v7</Name>
			<ConfigurationParameter>UseHelicityBasis:True</ConfigurationParameter>
			<ConfigurationParameter>AngularAcceptanceIgnoreNumerator:True</ConfigurationParameter>
			<ConfigurationParameter>UseTimeAcceptance:True</ConfigurationParameter>
			<ConfigurationParameter>UseEventResolution:False</ConfigurationParameter>
			<ConfigurationParameter>ResolutionModel:DummyResolutionModel</ConfigurationParameter>
		</PDF>
	</CommonPDF>

	<CommonPhaseSpace>
		<PhaseSpaceBoundary>
			<Observable>
				<Name>time</Name>
				<Minimum>0.3</Minimum>
				<Maximum>14.0</Maximum>
				<Unit>ps</Unit>
			</Observable>
			<Observable>
				<Name>helcosthetaK</Name>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>helphi</Name>
				<Minimum>-3.1416</Minimum>
				<Maximum>3.1416</Maximum>
				<Unit>rad</Unit>
			</Observable>
			<Observable>
				<Name>helcosthetaL</Name>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>tag</Name>
				<Value>0.</Value>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>mistag</Name>
				<Value>0.5</Value>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>eventResolution</Name>
				<Minimum>0.0</Minimum>
				<Maximum>0.12</Maximum>
				<Unit> </Unit>
			</Observable>
		</PhaseSpaceBoundary>
	</CommonPhaseSpace>

	<ToFit>
		<CommonPDF>True</CommonPDF>
		<PDFConfigurator>
			<ConfigurationParameter>TimeAcceptanceFile:MC2012_sim08e/all_time_efficiency_50bin_variable.txt</ConfigurationParameter>
		</PDFConfigurator>
		<DataSet>
			<Source>File</Source>
			<FileName>test.root</FileName>
			<NumberEvents>1000000</NumberEvents>

			<CommonPhaseSpace>
			</CommonPhaseSpace>

		</DataSet>
	</ToFit>

</RapidFit>


